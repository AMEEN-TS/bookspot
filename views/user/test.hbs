  <section class="confirmation_part padding_top">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">

          <div class="confirmation_tittle">
            <div class="d-flex justify-content-center pb-4 ">
              <img src="user/img/bg-img/accept.png" height="50px" alt="">
            </div>

            <span>Thank you. Your order has been received.</span>
          </div>
        </div>
        <div class="col-lg-12 col-lx-4">
          <div class="single_confirmation_details">
            <h4>order info</h4>
            <ul>
              <li>
                <p>order ID</p><span>:{{orderProducts._id}}</span>
              </li>
              <li>
                <p>Date</p><span>:{{orderProducts.ordered_on}}</span>
              </li>
              <li>
                <p>total</p><span>: {{orderProducts.grandTotal}}</span>
              </li>
              <li>
                <p>payment method</p><span>: {{orderProducts.paymentMethod}}</span>
              </li>
            </ul>
          </div>
        </div>

        <div class="col-lg-12 col-lx-4 mt-4">
          <div class="single_confirmation_details">
            <h4>shipping Address</h4>
            <ul>
              <li>
                <p>Name</p><span>: {{orderProducts.deliveryDetails.name}}</span>
              </li>
              <li>
                <p>Phone</p><span>: {{orderProducts.deliveryDetails.number}}</span>
              </li>
              <li>
                <p>Email</p><span>: {{orderProducts.deliveryDetails.email}}</span>
              </li>
              <li>
                <p>Address</p><span>:{{orderProducts.deliveryDetails.house}}</span>
                <span>,{{orderProducts.deliveryDetails.localplace}},</span>
                <span>{{orderProducts.deliveryDetails.town}},</span>
                <span>{{orderProducts.deliveryDetails.district}},</span>
                <span>{{orderProducts.deliveryDetails.state}}</span>
              </li>
              <li>
                <p>PINcode</p><span>: {{orderProducts.deliveryDetails.pincode}}</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="order_details_iner">
            <h3>Order Details</h3>
            <table class="table table-borderless">
              <thead>
                <tr>
                  <th scope="col" colspan="2">Product</th>
                  <th scope="col">Quantity</th>
                  <th scope="col">Total</th>
                </tr>
              </thead>

              <tbody>
                {{#each orderProducts.product}}
                <tr>
                  <th colspan="2"><span>{{pro_Id.productname}}</span></th>
                  <th>{{this.quantity}}</th>
                  <th> <span>Rs. {{this.subTotal}}</span></th>
                </tr>

                {{/each}}
              </tbody>
              <tfoot>
                <tr>
                  
                  <th scope="col">Grand Total : Rs. {{orderProducts.grandTotal}}</th>
                  
                  <th scope="col"><a class="btn_3" href="/orderPage">More details</a></th>
                </tr> 
                
              </tfoot>
            </table>
           
          </div>
        </div>
      </div>
    </div>
  </section>

